OUTPUT_FORMAT(elf32-i386)

ENTRY(_start)    /* Entry point for the kernel */

SECTIONS
{
    /* Kernel will be loaded at memory address 0x10000 */
    . = 0x10000;

    .text : {
        *(.text)    /* Put all the code in the .text section */
    }

    .data : {
        *(.data)    /* Put initialized data in the .data section */
    }

    .bss : {
        *(.bss)     /* Put uninitialized data in the .bss section */
    }
}